{
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@ui-config/*": [
        "./ui-config/*"
      ],
      "@ui-models/*": [
        "./ui-models/*"
      ],
      "@ui-lib/*": [
        "./ui-lib/*"
      ],
      "@config/*": [
        "./config/*"
      ],
      "react": [
        "./node_modules/@types/react"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "additional.d.ts",
    "meet.ts",
    "src/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
